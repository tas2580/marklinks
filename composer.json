	"description": "Mark externail links with an icon.",
	"homepage": "https://tas2580.net/downloads/phpbb-marklinks/",
	"version": "0.2.2",
	"keywords": ["phpbb", "extension", "links"],
	"license": "GPL-2.0",
	"authors": [{
			"name": "tas2580",
			"email": "mail@tas2580.net",
